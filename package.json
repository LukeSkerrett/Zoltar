{
  "name": "zoltar",
  "version": "0.1.0",
  "description": "Site: https://cse110-sp23-group23.github.io/Zoltar/",
  "type": "module",
  "scripts": {
    "test": "jest --passWithNoTests",
    "lint:js": "eslint assets/scripts/**/*.js __tests__/**/*.js",
    "lint": "npm run lint:js",
    "start": "serve -l 5500 ."
  },
  "jest": {
    "verbose": true,
    "preset": "jest-puppeteer",
    "testEnvironment": "jest-environment-jsdom",
    "transform": {}
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "indent": [
        "error",
        "tab"
      ],
      "no-tabs": "off",
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "js": "always"
        }
      ],
      "import/prefer-default-export": "off",
      "max-len": [
        "error",
        {
          "code": 120
        }
      ]
    },
    "env": {
      "browser": true,
      "jest": true
    }
  },
  "devDependencies": {
    "@percy/cli": "^1.25.0",
    "@percy/core": "^1.25.0",
    "@percy/puppeteer": "^2.0.2",
    "core-js": "^3.30.2",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-puppeteer": "^9.0.0",
    "node-fetch": "^3.3.1",
    "percy": "^5.0.0",
    "puppeteer": "^20.5.0",
    "serve": "^14.2.0"
  }
}
